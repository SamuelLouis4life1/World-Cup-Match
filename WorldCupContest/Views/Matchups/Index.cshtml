@model IEnumerable<WorldCupContest.Models.Matchup>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TeamQuantity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MatchupDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstTeamName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SecondTimeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TeamQuantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MatchupDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstTeamName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SecondTimeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
